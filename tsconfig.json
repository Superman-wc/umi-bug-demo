    //    "moduleResolution": "node",
    //    "allowSyntheticDefaultImports": true,
    //    "experimentalDecorators": true,
    //    "jsx": "preserve",
    //    "target": "es6",
    //    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
